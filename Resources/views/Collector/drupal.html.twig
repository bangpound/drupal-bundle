{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
    <img width="16" height="24" alt="Drufony" src="data:image/png;base64," />
    {% endset %}
    {% set text %}
    <div class="sf-toolbar-info-piece">
        <b>Bootstrap phase</b>
        <span>{{ collector.bootstrapPhase }}</span>
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Base URL</b>
        <span>{{ collector.baseUrl }}</span>
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Base path</b>
        <span>{{ collector.basePath }}</span>
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Base root</b>
        <span>{{ collector.baseRoot }}</span>
    </div>
    <div class="sf-toolbar-info-piece">
        <b>conf_path</b>
        <span>{{ collector.confPath }}</span>
    </div>
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt="Drupal"></span>
        <strong>Drupal</strong>
    </span>
{% endblock %}

{% block panel %}
    {% if collector.drupal %}
        {% if collector.requirements %}
            <h2>Requirements</h2>
            <table>
                {% for requirement in collector.requirements %}
                    <tr>
                        <th class="{% if requirement.severity is defined %}{{ requirement.severity }}{% endif %}">{{ requirement.title }}</th>
                        <td>{{ requirement.value | raw }}</td>
                    </tr>
                {% endfor %}
            </table>
        {% endif %}
        {% if collector.globals %}
            <h2>Global variables</h2>
            <table>
                <tr>
                    <th>Name</th>
                    <th>Path</th>
                </tr>
                {% for key in collector.globals|keys|sort %}
                    <tr>
                        <th>{{ key | e }}</th>
                        <td>{{ collector.globals[key] | e }}</td>
                    </tr>
                {% endfor %}
            </table>
        {% endif %}
        {% if collector.conf %}
            <h2>Drupal variables</h2>
            <table>
                <tr>
                    <th>Name</th>
                    <th>Path</th>
                </tr>
                {% for key in collector.conf|keys|sort %}
                    <tr>
                        <th>{{ key | e }}</th>
                        <td>{{ collector.conf[key] | e }}</td>
                    </tr>
                {% endfor %}
            </table>
        {% endif %}
    {% endif %}
{% endblock %}
